{
  "name": "serverless-practice",
  "description": "",
  "version": "0.1.0",
  "scripts": {
    "webpack-gen-staging": "serverless webpack --stage staging",
    "webpack-gen-prod": "NODE_ENV=prod serverless webpack --stage prod",
    "start-offline": "serverless offline --stage staging",
    "debug-offline": "node --inspect ./node_modules/.bin/serverless offline --stage staging",
    "deploy-staging": "serverless deploy --stage staging",
    "deploy-prod": "serverless deploy --stage prod",
    "eslint": "eslint --fix --ext .js .",
    "test-staging": "sls invoke test --stage staging --compilers js:babel-core/register",
    "check-security": "npm audit"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "aws-sdk": "^2.519.0",
    "dotenv": "^6.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "serverless": "^1.50.1",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.10.1",
    "serverless-webpack": "^5.2.0",
    "terser-webpack-plugin": "^1.0.2",
    "webpack": "^4.17.1",
    "webpack-node-externals": "^1.7.2"
  }
}
